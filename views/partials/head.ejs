        "addressLocality": "Karachi",
        "addressCountry": "Pakistan"
      },
      "contactPoint": {
        "@type": "ContactPoint",
        "telephone": "+92-XXX-XXXXXXX",
        "contactType": "customer service",
        "email": "info@softedigi.com"
      },
      "sameAs": [
        "https://www.facebook.com/softedigi",
        "https://www.linkedin.com/company/softedigi",
