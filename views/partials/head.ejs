<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- SEO Meta Tags -->
    <title><%= title || 'Softedigi - Professional Software Development & Digital Solutions' %></title>
    <meta name="description" content="<%= description || 'Softedigi delivers cutting-edge software development, web design, and digital marketing solutions. Transform your business with our innovative IT services in Pakistan.' %>">
    <meta name="keywords" content="<%= keywords || 'software development, web development, digital marketing, IT solutions, Pakistan, Karachi, mobile apps, custom software' %>">
    <meta name="author" content="Softedigi">
    <meta name="robots" content="index, follow">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="https://www.softedigi.com<%= canonicalUrl || '' %>">
    
    <!-- Open Graph Meta Tags -->
    <meta property="og:title" content="<%= title || 'Softedigi - Professional Software Development' %>">
    <meta property="og:description" content="<%= description || 'Transform your business with innovative software solutions and digital services.' %>">
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://www.softedigi.com<%= canonicalUrl || '' %>">
    <meta property="og:image" content="https://www.softedigi.com/images/og-image.jpg">
    <meta property="og:site_name" content="Softedigi">
    <meta property="og:locale" content="en_US">
    
    <!-- Twitter Card Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= title || 'Softedigi - Professional Software Development' %>">
    <meta name="twitter:description" content="<%= description || 'Transform your business with innovative software solutions.' %>">
    <meta name="twitter:image" content="https://www.softedigi.com/images/twitter-card.jpg">
    
    <!-- Schema.org JSON-LD -->
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "Organization",
      "name": "Softedigi",
      "url": "https://www.softedigi.com",
      "logo": "https://www.softedigi.com/images/logo.png",
      "description": "Professional software development and digital solutions company",
      "address": {
        "@type": "PostalAddress",
        "streetAddress": "402, 4th floor, anum blessing, 13 Shahrah e faisal",
        "addressLocality": "Karachi",
        "addressCountry": "Pakistan"
      },
      "contactPoint": {
        "@type": "ContactPoint",
        "telephone": "+92-XXX-XXXXXXX",
        "contactType": "customer service",
        "email": "info@softedigi.com"
      },
      "sameAs": [
        "https://www.facebook.com/softedigi",
        "https://www.linkedin.com/company/softedigi",
        "https://twitter.com/softedigi"
      ]
    }
    </script>
    
    <!-- Bootstrap 5 CSS CDN -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Bootstrap Icons -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
    <link rel="stylesheet" href="/css/index.css">
</head>