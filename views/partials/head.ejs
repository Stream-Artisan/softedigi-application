<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- Favicon -->
    <link rel="icon" type="image/x-icon" href="/favicon.ico">
    <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
    
    <!-- SEO Meta Tags -->
    <title><%= title || 'Softedigi - Professional Software Development & Digital Solutions' %></title>
    <meta name="description" content="<%= description || 'Softedigi provides innovative software development, web design, and digital marketing solutions to transform your business with cutting-edge IT services in Pakistan' %>">
    <meta name="keywords" content="<%= keywords || 'software development, web development, digital marketing, IT solutions, Pakistan, Karachi, mobile apps, custom software' %>">
    <meta name="author" content="Softedigi">
    <meta name="robots" content="index, follow">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="https://www.softedigi.com<%= canonicalUrl || '' %>">
    
    <!-- Open Graph Meta Tags -->
    <meta property="og:title" content="<%= title || 'Softedigi - Professional Software Development' %>">
    <meta property="og:description" content="<%= description || 'Transform your business with innovative software solutions and digital services.' %>">
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://www.softedigi.com<%= canonicalUrl || '' %>">
    <meta property="og:image" content="https://www.softedigi.com/images/og-image.jpg">
    <meta property="og:site_name" content="Softedigi">
    <meta property="og:locale" content="en_US">
    
    <!-- Twitter Card Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= title || 'Softedigi - Professional Software Development' %>">
    <meta name="twitter:description" content="<%= description || 'Transform your business with innovative software solutions.' %>">
    <meta name="twitter:image" content="https://www.softedigi.com/images/twitter-card.jpg">
    
