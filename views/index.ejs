    <!-- contact us section -->
    <section class="section contact-section py-5" id="contact">
      <div class="container">
        <div class="row">
          <div class="col-md-6 section-title">
            <h2 class="mb-4">Don't hesitate to contact us</h2>
            <form action="send_email.php" method="POST" class="appointment">
              <div id="form-messages"></div>
              <input type="hidden" name="csrf_token" id="csrf_token" />
              <div class="row">
                <div class="col-md-6">
                  <div class="mb-3">
                    <input
                      type="text"
                      class="form-control"
                      name="name"
                      placeholder="Your Name"
                      required
                      maxlength="100"
                    />
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3">
                    <input
                      type="email"
                      class="form-control"
                      name="email"
                      placeholder="Email"
                      required
                      maxlength="254"
                    />
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3">
                    <input
                      type="tel"
                      class="form-control"
                      name="number"
                      placeholder="Phone Number"
                      required
                      pattern="[0-9+\-\s()]{6,20}"
                    />
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="mb-3">
                    <select
                      name="service"
                      id="service"
                      class="form-select"
                      required
                    >
                      <option value="">Select Service</option>
                      <option value="Web Development">Web Development</option>
                      <option value="Graphic Designing">
                        Graphic Designing
                      </option>
                      <option value="Mobile Applications">
                        Mobile Applications
                      </option>
                      <option value="UX/UI Strategy">UX/UI Strategy</option>
                      <option value="Digital Marketing">
                        Digital Marketing
                      </option>
                      <option value="Web Applications">Web Applications</option>
                      <option value="E-Book Services">E-Book Services</option>
                    </select>
                  </div>
                </div>
                <div class="col-md-12">
                  <div class="mb-3">
                    <textarea
                      name="message"
                      id="message"
                      rows="7"
                      class="form-control"
                      placeholder="Message"
                      required
                      maxlength="3000"
                    ></textarea>
                  </div>
                </div>
                <div class="col-md-12">
                  <div class="mb-3">
                    <input
                      type="submit"
                      value="Send message"
                      class="btn btn-primary py-3 px-4"
                    />
                  </div>
                </div>
              </div>
            </form>
          </div>
          <div class="col-md-6">
            <div class="h-100 d-flex flex-column justify-content-center">
              <div class="map-container">
                <iframe
                  src="https://maps.app.goo.gl/8YLT7sPc2hwymAQz8"
                  width="100%"
                  height="300"
                  style="border: 0; border-radius: 8px"
                  allowfullscreen=""
                  loading="lazy"
                ></iframe>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
